(\n            ans += item.amount * item.price\n        ))\n        setPrice(ans);\n    }\n\n    const handleRemove = (id) =>{\n        const arr = cart.filter((item)=>item.id !== id);\n        setCart(arr);\n        // handlePrice();\n    }\n\n    useEffect(()=>{\n        handlePrice();\n    })\n\n  return (\n    <article>\n        {\n            cart?.map((item)=>(\n                <div className=\"cart_box\" key={item.id}>\n                    <div className=\"cart_img\">\n                        <img src={item.img} />\n                        <p>{item.title}</p>\n                    </div>\n                    <div>\n                        <button onClick={()=>handleChange(item, +1)}> + </button>\n                        <button>{item.amount}</button>\n                        <button onClick={()=>handleChange(item, -1)}> - </button>\n                    </div>\n                    <div>\n                       \n                        <span>Rs. {item.price}</span>\n                        <button onClick={()=>handleRemove(item.id)} >Remove</button>\n                    </div>\n                </div>\n            ))}\n        <div className='total'>\n            <span>Total Price of your Cart</span>\n            <span>Rs - {price}</span>\n        </div>\n    </article>\n  )\n}\n\nexport default Cart","C:\\Users\\thapa\\OneDrive\\Desktop\\Assignment\\Copy-bizbazar\\src\\data.js",[],[],"C:\\Users\\thapa\\OneDrive\\Desktop\\Assignment\\Copy-bizbazar\\src\\components\\Cards.js",["79"],[],"import React from 'react';\nimport '../styles/cards.css'\n\nconst Cards = ({item, handleClick}) => {\n    const {title, author, price, img} = item;\n  return (\n    <div className=\"cards\">\n        <div className=\"image_box\">\n            <img src={img} alt=\"Image\" />\n        </div>\n        <div className=\"details\">\n            <p>{title}</p>\n            <p>{author}</p>\n            <span className='discount'><del>Rs. 2000</del></span>\n            <p>Price - {price}Rs